		WG12864A_posXY(13, 7);
		WG12864A_printf(" S ", Arial8x6, BLANCO);
		delay32Ms(0, TIMMER_FONDO_BLANCO);
		adc_valX = 0, adc_valY = 0;					// Reseteo el valor de X, Y del ADC.
